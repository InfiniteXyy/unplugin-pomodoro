{
  "name": "unplugin-pomodoro",
  "version": "0.0.3",
  "description": "A vite plugin, brings pomodoro to your development flow",
  "keywords": [
    "unplugin",
    "pomodoro",
    "vite"
  ],
  "homepage": "https://github.com/InfiniteXyy/unplugin-pomodoro#readme",
  "bugs": {
    "url": "https://github.com/InfiniteXyy/unplugin-pomodoro/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/InfiniteXyy/unplugin-pomodoro.git"
  },
  "license": "MIT",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./vite": {
      "require": "./dist/vite.js",
      "import": "./dist/vite.mjs"
    },
    "./types": {
      "require": "./dist/types.js",
      "import": "./dist/types.mjs"
    },
    "./*": "./*"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "*.d.ts"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch src",
    "build:post": "esno scripts/postbuild.ts && pnpm run build --filter pomodoro-widget && cp -R ./widget/dist/* ./dist",
    "lint": "eslint \"{src,test}/**/*.ts\"",
    "lint:fix": "npm run lint -- --fix",
    "play": "npm -C playground run dev",
    "prepublishOnly": "npm run build",
    "release": "npx bumpp --commit --push --tag && pnpm publish",
    "start": "esno src/index.ts",
    "test": "jest",
    "test:update": "jest -u"
  },
  "dependencies": {
    "unplugin": "^0.2.7"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.7.0",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.10",
    "eslint": "^7.32.0",
    "eslint-plugin-jest": "^24.4.0",
    "esno": "^0.9.1",
    "fast-glob": "^3.2.7",
    "jest": "^27.1.0",
    "nodemon": "^2.0.12",
    "prettier": "^2.6.2",
    "prettier-plugin-tailwindcss": "^0.1.8",
    "rimraf": "^3.0.2",
    "rollup": "^2.56.3",
    "ts-jest": "^27.0.5",
    "tsup": "^4.14.0",
    "typescript": "^4.4.2",
    "vite": "^2.5.3",
    "webpack": "^5.51.2"
  }
}
